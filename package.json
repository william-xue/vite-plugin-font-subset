{
  "name": "@fe-fast/vite-plugin-font-subset",
  "version": "0.1.2",
  "description": "Vite plugin to subset fonts to WOFF2 based on project character usage",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "font",
    "subset",
    "woff2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/william-xue/vite-plugin-font-subset.git"
  },
  "homepage": "https://github.com/william-xue/vite-plugin-font-subset#readme",
  "bugs": {
    "url": "https://github.com/william-xue/vite-plugin-font-subset/issues"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "subset-font": "^2.4.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
